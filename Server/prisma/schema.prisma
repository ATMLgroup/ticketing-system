generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Customers {
  id          Int      @id @default(autoincrement())
  email       String   @unique
  password    String
  createdAt   DateTime @default(now())
  firstName   String
  lastName    String
  phoneNumber String
}
